<md-input-container>
  <%-await template('form/title')%>
  <%-await template('form/description')%>
  <checkboxes layout="column"
    name="<%-field.name%>"
    ng-model="<%-field.model%>"
    <%-formValidators(field)%>>
    <%for(var option of field.options){

  option.model = field.model+'.'+option.value;

  %>
    <%-await template('form/checkbox',{field:option})%>
    <%}%>
  </checkboxes>
  <%-await template('form/messages')%>
</md-input-container>